{
	"name": "Node.js",
	"build": {
		"dockerfile": "Dockerfile",		
		"args": { "VARIANT": "14" }
	},
	"settings": {},
	"extensions": [
		"dbaeumer.vscode-eslint",
		"orta.vscode-jest",
		"firsttris.vscode-jest-runner"
	],	
	"forwardPorts": [3000],
	"portsAttributes": {
		"3000": {
			"label": "Node Remote Port",
			"onAutoForward": "notify"
		}
	},	
	"postCreateCommand": "npm install"
}